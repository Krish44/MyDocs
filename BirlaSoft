SELECT CITY, COUNT(1) FROM T_LOCATION GROUP BY CITY WHERE CITY LIKE '%SOUTH';

SELECT CITY, COUNT(1) CNT FROM T_LOCATION GROUP BY CITY WHERE CNT > 5;

